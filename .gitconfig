#
# .gitconfig
#
[user]
    name = masartz
    email = masartz@gmail.com
[alias]
    co = checkout
    st = status
    home = checkout master
    rmb = branch -D
    bra = branch
[fetch]
    prune = true
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
# gitlab設定
[http]
	sslVerify = true
[core]
	editor = vim
